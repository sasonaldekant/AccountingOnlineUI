#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/mgasic/Documents/AIProjects/AccountingOnline/AccountingOnlineUI/node_modules/sucrase/bin/node_modules:/mnt/c/Users/mgasic/Documents/AIProjects/AccountingOnline/AccountingOnlineUI/node_modules/sucrase/node_modules:/mnt/c/Users/mgasic/Documents/AIProjects/AccountingOnline/AccountingOnlineUI/node_modules:/mnt/c/Users/mgasic/Documents/AIProjects/AccountingOnline/node_modules:/mnt/c/Users/mgasic/Documents/AIProjects/node_modules:/mnt/c/Users/mgasic/Documents/node_modules:/mnt/c/Users/mgasic/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/mgasic/Documents/AIProjects/AccountingOnline/AccountingOnlineUI/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/mgasic/Documents/AIProjects/AccountingOnline/AccountingOnlineUI/node_modules/sucrase/bin/node_modules:/mnt/c/Users/mgasic/Documents/AIProjects/AccountingOnline/AccountingOnlineUI/node_modules/sucrase/node_modules:/mnt/c/Users/mgasic/Documents/AIProjects/AccountingOnline/AccountingOnlineUI/node_modules:/mnt/c/Users/mgasic/Documents/AIProjects/AccountingOnline/node_modules:/mnt/c/Users/mgasic/Documents/AIProjects/node_modules:/mnt/c/Users/mgasic/Documents/node_modules:/mnt/c/Users/mgasic/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/mgasic/Documents/AIProjects/AccountingOnline/AccountingOnlineUI/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../sucrase/bin/sucrase" "$@"
else
  exec node  "$basedir/../sucrase/bin/sucrase" "$@"
fi
